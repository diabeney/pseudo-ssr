{
  "devDependencies": {
    "@babel/preset-react": "^7.22.15",
    "esbuild": "0.19.3",
    "jest": "^29.3.1",
    "mocha": "^10.2.0"
  },
  "scripts": {
    "test": "jest",
    "demo": "node index.js",
    "build:client": "esbuild client.jsx  --jsx=automatic --format=esm --bundle --outfile=.smooth/client.js",
    "build:dist": "esbuild app/**/page.jsx --format=cjs  --jsx=automatic  --bundle --outdir=.smooth/app --packages=external",
    "build:server": "esbuild  server.jsx --jsx=automatic --bundle --platform=node   --outfile=.smooth/server.js --packages=external",
    "build": "npm run build:client && npm run build:dist && npm run build:server",
    "start": "node .smooth/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
